{
  "name": "adgenius-innovation-hub",
  "version": "1.0.0",
  "description": "AI-Powered Retail Media Creative Tool for Tesco InnovAItion Jam",
  "private": true,
  "workspaces": [
    "ai-engine",
    "backend",
    "frontend",
    "shared"
  ],
  "scripts": {
    "dev:ai": "npm run dev --workspace=ai-engine",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "test": "npm test --workspaces"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
